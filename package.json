{
  "name": "test-relay-experimental",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@forminator/react-wire": "^0.4.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-relay": "^11.0.3",
    "@types/relay-runtime": "^12.0.1",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-relay": "^12.0.0",
    "react-scripts": "4.0.3",
    "relay-runtime": "^12.0.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "get-schema": "graphql-codegen -p schema",
    "relay:enums": "graphql-codegen -p enums",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions ts tsx --language typescript && yarn relay:enums",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000/",
  "devDependencies": {
    "@graphql-cli/codegen": "^2.1.1",
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/schema-ast": "^2.1.0",
    "@graphql-codegen/typescript": "^2.1.1",
    "babel-plugin-relay": "^12.0.0",
    "graphql": "^15.0.0",
    "prettier": "^2.5.1",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^15.0.0",
    "relay-config": "^12.0.1"
  }
}
